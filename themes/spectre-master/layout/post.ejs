<!-- Layouts -->
<% if(page.layout === 'tags') { %>
    <!-- Tags Cloud -->
    <%- partial('_pages/tagcloud') %>
<% } else if(page.layout === 'links') { %>
    <!-- Single Links Page -->
    <%- partial('_pages/links') %>
<% } else if(page.layout === 'timeline') { %>
    <!-- Single Timeline Page -->
    <%- partial('_pages/timeline') %>
<% } else if(page.layout === 'search') { %>
    <!-- Local Search Page -->
    <%- partial('_pages/search') %>
<% } else { %>
    <!-- Post Moudule -->
    <%- partial('_pages/post') %>
<% } %>
