<meta http-equiv="x-dns-prefetch-control" content="on">
<% if(theme.vendors.materialcdn) { %>
    <link rel="dns-prefetch" href="<%= theme.vendors.materialcdn %>"/>
<% } %>
<% if( (theme.leancloud.enable === true) || (theme.comment.use == "valine") ) { %>
    <link rel="dns-prefetch" href="https://cdn1.lncld.net"/>
<% } %>
<% if(theme.busuanzi.enable === true) { %>
    <link rel="dns-prefetch" href="https://busuanzi.ibruce.info"/>
<% } %>

<% if(theme.comment.use === "changyan") { %>
    <link rel="dns-prefetch" href="https://changyan.sohu.com"/>
<% } else if(theme.comment.use === "livere") { %>
    <link rel="dns-prefetch" href="https://cdn-city.livere.com"/>
<% } else if(theme.comment.use === "gitment") { %>
    <link rel="dns-prefetch" href="https://imsun.github.io"/>
<% } else if(theme.comment.use === "valine") { %>
    <link rel="dns-prefetch" href="https://cdn1.lncld.net"/>
    <link rel="dns-prefetch" href="https://fastly.jsdelivr.net"/>
<% } else if(theme.comment.use && theme.comment.use.startsWith("disqus")) { %>
    <link rel="dns-prefetch" href="https://<%= theme.comment.shortname %>.disqus.com"/>
<% } %>

<% if(theme.analytics.baidu_site_id) { %>
    <link rel="dns-prefetch" href="https://hm.baidu.com"/>
<% } if(theme.analytics.google_site_id) { %>
    <link rel="dns-prefetch" href="https://www.google-analytics.com"/>
<% } if(theme.analytics.cnzz_site_id) { %>
    <link rel="dns-prefetch" href="https://s95.cnzz.com"/>
<% } %>

<% if(theme.fonts.use === "google") { %>
    <link rel="dns-prefetch" href="https://fonts.googleapis.com"/>
<% } else if(theme.fonts.use === "ustc") { %>
    <link rel="dns-prefetch" href="https://fonts.proxy.ustclug.org"/>
<% } else if(theme.fonts.use === "baomitu") { %>
    <link rel="dns-prefetch" href="https://lib.baomitu.com"/>
<% } else if(theme.fonts.use === "custom") { %>
    <link rel="dns-prefetch" href="<%= theme.fonts.custom_font_host %>"/>
<% } else if(theme.fonts.use === "catnet") { %>
    <link rel="dns-prefetch" href="https://cdnjs.loli.net"/>
<% } %>

<% if(theme.qrcode.use === "online" && theme.qrcode.enable === "true") { %>
    <% if(config.language === "zh-CN") { %>
        <link rel="dns-prefetch" href="https://pan.baidu.com"/>
    <% } else { %>
        <link rel="dns-prefetch" href="https://chart.googleapis.com"/>
    <% } %>
<% } %>
